<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProjektX</title>
    <link rel="icon" type="image/x-icon" href="~/lib/bootstraplayout/assets/favicon.ico" />
     <!--Font Awesome icons (darmowa wersja)-->
    <script src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" crossorigin="anonymous"></script>
    <!--Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" />
    <!--CSS-->
     <link href="~/lib/bootstraplayout/css/styles.css" rel="stylesheet" />
</head>
<body id="page-top">
        <!-- Nawigacja-->
        <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
           <div class="container px-4 px-lg-5">
                @*<a class="navbar-brand" href="#page-top">TUMAK</a>*@
                <button class="navbar-toggler navbar-toggler-right" type="button" data-bs-toggle="collapse" data-bs-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                    Menu
                    <i class="fas fa-bars"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarResponsive">
                 @await Html.PartialAsync("NavView",(IEnumerable<KoloTumak.Data.Data.CMS.EditWWW>)ViewBag.ModelNavView)   
<a class="btn btn-primary" href="#about">Logowanie</a>
</div>
</div> 
        </nav>
        <!-- Środkowy kontent-->
        <header class="masthead">
            <div class="container px-4 px-lg-5 d-flex h-100 align-items-center justify-content-center">
                <div class="d-flex justify-content-center">
                    <div class="text-center">
                        <h1 class="mx-auto my-0 text-uppercase">



                        </h1>                                               
                    </div>
                </div>
            </div>
        </header>
        @RenderBody()
        <!--O NAS-->
        <section class="about-section text-center" id="about">
            <div class="container px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    @await Html.PartialAsync("AboutView",(IEnumerable<KoloTumak.Data.Data.CMS.About>)ViewBag.ModelAbout)
                </div>             
            </div>
        </section>
        <!--Zarząd-->
        <section class="about-section text-center" id="projects">

         @await Html.PartialAsync("ManagementView",(IEnumerable<KoloTumak.Data.Data.CMS.Management>)ViewBag.ModelManagement)

        </section>
        <!--Login-->
        
        <!--Kontakt-->
        <section class="contact-section bg-black">
            <div class="container px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5">
                    <div class="col-md-4 mb-3 mb-md-0">
                        <div class="card py-4 h-100">
                            <div class="card-body text-center">
                                <i class="fas fa-map-marked-alt text-primary mb-2"></i>
                                <h4 class="text-uppercase m-0">Adres</h4>
                                <hr class="my-4 mx-auto" />
                                <div class="small text-black-50">Ciechanów.........</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 mb-3 mb-md-0">
                        <div class="card py-4 h-100">
                            <div class="card-body text-center">
                                <i class="fas fa-envelope text-primary mb-2"></i>
                                <h4 class="text-uppercase m-0">Email</h4>
                                <hr class="my-4 mx-auto" />
                                <div class="small text-black-50"><a href="#!">hello@yourdomain.com</a></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 mb-3 mb-md-0">
                        <div class="card py-4 h-100">
                            <div class="card-body text-center">
                                <i class="fas fa-mobile-alt text-primary mb-2"></i>
                                <h4 class="text-uppercase m-0">Telefon</h4>
                                <hr class="my-4 mx-auto" />
                                <div class="small text-black-50">xxx-xxx-xxx</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--Stopka-->
        <footer class="footer bg-black small text-center text-white-50"><div class="container px-4 px-lg-5">Copyright &copy; Koło Łowieckie Tumak Ciechanów</div></footer>
        <script src="js/scripts.js"></script>
        @*<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>       
        <script src="https://cdn.startbootstrap.com/sb-forms-latest.js"></script>*@
        <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>
